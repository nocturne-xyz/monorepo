{
  "name": "@nocturne-xyz/bundler",
  "version": "1.0.0-alpha",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "scripts": {
    "build": "tsc --build",
    "check": "tsc --noEmit",
    "clean": "rm -rf .turbo dist",
    "lint": "eslint --fix src --ext .ts",
    "test:unit": "mocha --require ts-node/register test/** --timeout 100000 --exit",
    "run:server": "npx ts-node ./scripts/runServer --walletAddress ",
    "run:batcher": "npx ts-node ./scripts/runBatcher",
    "run:submitter": "npx ts-node ./scripts/runSubmitter --walletAddress ",
    "prettier:check": "prettier --check ./src ./test",
    "prettier:write": "prettier --write ./src ./test"
  },
  "dependencies": {
    "@nocturne-xyz/contracts": "workspace:^",
    "@nocturne-xyz/sdk": "workspace:^",
    "@types/express": "^4.17.14",
    "ajv": "^8.11.2",
    "bigint-json-serialization": "^1.0.1",
    "bullmq": "^3.4.1",
    "ethers": "^5.0.0",
    "express": "^4.18.2",
    "ioredis": "^5.2.4"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.1",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.8.0",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "chai": "^4.3.6",
    "dotenv": "^16.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "js-sha256": "^0.9.0",
    "mocha": "^10.1.0",
    "prettier": "^2.8.0",
    "redis-memory-server": "^0.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
