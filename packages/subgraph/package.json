{
  "name": "@nocturne-xyz/subgraph",
  "license": "UNLICENSED",
  "scripts": {
    "codegen": "graph codegen",
    "build": "yarn codegen && graph build",
    "deploy-hosted-service": "graph deploy --node https://api.thegraph.com/deploy/ nocturne-test",
    "create-local": "graph create --node http://localhost:8020/ nocturne-test",
    "remove-local": "graph remove --node http://localhost:8020/ nocturne-test",
    "deploy-local": "echo '\n' | graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 nocturne-test",
    "prettier:check": "prettier --check ./src",
    "prettier:write": "prettier --write './src'"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.37.7",
    "@graphprotocol/graph-ts": "0.29.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^7.20.0",
    "matchstick-as": "0.5.0",
    "prettier": "^2.8.0"
  }
}
