{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "lint": {
      "outputs": []
    },
    "build": {
      "dependsOn": ["lint"],
      "outputs": ["dist/**"]
    },
    "@flax/sdk#build": {
      "dependsOn": ["@flax/contracts#build"]
    },
    "@flax/e2e-tests#build": {
      "dependsOn": ["@flax/contracts#build", "@flax/sdk#build"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "test:unit": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "test:e2e": {
      "dependsOn": ["build"],
      "outputs": []
    }
  }
}
